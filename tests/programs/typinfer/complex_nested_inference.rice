fn process_complex(data: _) -> _ {
    let result = 0 in
    let i = 0 in
    while i < 2 {
        let j = 0 in
        while j < 2 {
            let pair = data[i][j] in
            if pair.0 > 0 {
                result := result + pair.1
            } else {
                result := result - pair.1
            };
            j := j + 1
        };
        i := i + 1
    };
    result
}

fn create_matrix() -> _ {
    [[(1, 10), (2, 20)], [(-1, 30), (3, 40)]]
}

fn main() {
    let matrix = create_matrix() in
    let sum = process_complex(matrix) in
    println(int_to_string(sum))
}
