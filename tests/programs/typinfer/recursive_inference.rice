fn factorial(n: _) -> _ {
    if n <= 1 {
        1
    } else {
        n * factorial(n - 1)
    }
}

fn fibonacci(n: _) -> _ {
    if n <= 1 {
        n
    } else {
        fibonacci(n - 1) + fibonacci(n - 2)
    }
}

fn main() {
    let fact = factorial(5) in
    let fib = fibonacci(6) in
    println(int_to_string(fact));
    println(int_to_string(fib))
}
