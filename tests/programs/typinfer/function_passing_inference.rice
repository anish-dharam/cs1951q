fn apply_function(f: _, x: _) -> _ {
    f(x)
}

fn add_one(x: int) -> int {
    x + 1
}

fn multiply_by_two(x: int) -> int {
    x * 2
}

fn main() {
    let result1 = apply_function(add_one, 5) in
    let result2 = apply_function(multiply_by_two, 7) in
    println(int_to_string(result1));
    println(int_to_string(result2))
}
