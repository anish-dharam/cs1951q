fn process_nested(arr: _) -> _ {
    let result = 0 in
    let i = 0 in
    while i < 2 {
        let j = 0 in
        while j < 2 {
            result := result + arr[i][j];
            j := j + 1
        };
        i := i + 1
    };
    result
}

fn get_nested_element(matrix: _, row: _, col: _) -> _ {
    matrix[row][col]
}

fn main() {
    let matrix: [[int]] = [[1, 2], [3, 4]] in
    let sum = process_nested(matrix) in
    let element = get_nested_element(matrix, 0, 1) in
    println(int_to_string(sum));
    println(int_to_string(element))
}
